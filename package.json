{
  "name": "deep-consulting",
  "version": "1.0.0",
  "description": "Back End Test for Deep Consulting",
  "main": "build/index.js",
  "repository": "https://github.com/t-kings/node-express-ts.git",
  "author": "Nwachukwu Tochukwu <github.com/tkings>",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-form-data": "^2.0.2",
    "@types/jest": "^27.0.2",
    "@types/mysql": "^2.15.19",
    "@types/node": "^16.11.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "start:dev": "nodemon",
    "start": "yarn build && node build/index.js",
    "start:debug": "nodemon --inspect",
    "start:prod": "node build/index.js",
    "build": "tsc",
    "lint": "eslint . --ext .ts --fix",
    "lint:format": "yarn format && yarn lint",
    "prepare": "husky install",
    "format": "prettier --config .prettierrc src/**/*.ts --write",
    "test": "jest --config ./jest-e2e.json --passWithNoTests ",
    "test:watch": "jest --watch --passWithNoTests",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:e2e": "jest --config ./test/jest-e2e.json --passWithNoTests --detectOpenHandles",
    "supertest": "^6.1.6"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mysql": "^2.18.1"
  }
}
